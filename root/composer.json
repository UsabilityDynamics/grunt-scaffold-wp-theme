{
  "name": "{%= name %}",
  "version": "{%= version %}",
  "type": "theme",
  "description": "{%= description %}",
  "homepage": "{%= homepage %}",
  "private": true,
  "author": {
    "name": "{%= author_name %}",
    "url": "{%= author_url %}"
  },
  "license": "{%= license %}",
  "license_url": "{%= license_url %}",
  "tags": [ "one-column", "two-column", "translation-ready", "responsive", "wordpress", "theme" ],
  "repository": {
    "type": "git",
    "url": "https://github.com/UsabilityDynamics/{%= short_name %}.git"
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:UsabilityDynamics/lib-settings.git",
      "trunk-path": "trunk",
      "branches-path": "branches",
      "tags-path": "tags"
    },
    {
      "type": "git",
      "url": "git@github.com:UsabilityDynamics/lib-ui.git",
      "trunk-path": "trunk",
      "branches-path": "branches",
      "tags-path": "tags"
    }
  ],
  "require": {
    "UsabilityDynamics/lib-settings": "*",
    "UsabilityDynamics/lib-ui": "dev-master"
  }
}
